{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*)\\.(js|css|jpg|jpeg|png|gif|ico|html|json)$",
            "dest": "/dist/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ]
}