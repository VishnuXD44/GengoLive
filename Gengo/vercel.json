{
    "version": 2,
    "builds": [
        {
            "src": "dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/socket.io/:path*",
            "destination": "https://gengolive-production.up.railway.app/socket.io/:path*"
        },
        {
            "source": "/assets/:path*",
            "destination": "/dist/assets/:path*"
        },
        {
            "source": "/styles.css",
            "destination": "/dist/styles.css"
        },
        {
            "source": "/styles2.css",
            "destination": "/dist/styles2.css"
        },
        {
            "source": "/:path*.js",
            "destination": "/dist/:path*.js"
        },
        {
            "source": "/main",
            "destination": "/dist/main.html"
        },
        {
            "source": "/about",
            "destination": "/dist/about.html"
        },
        {
            "source": "/((?!socket.io|assets|dist).*)",
            "destination": "/dist/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, PUT, DELETE, PATCH, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "X-Requested-With, X-HTTP-Method-Override, Content-Type, Accept, Authorization"
                },
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": "false"
                },
                {
                    "key": "Access-Control-Max-Age",
                    "value": "86400"
                }
            ]
        }
    ],
    "regions": ["all"],
    "cleanUrls": true,
    "trailingSlash": false
}