{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "src/server.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,HEAD,PUT,PATCH,POST,DELETE",
                "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept",
                "Access-Control-Allow-Credentials": "true"
            }
        },
        {
            "src": "/main.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/dist/main.js"
        },
        {
            "src": "/bundle.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/dist/bundle.js"
        },
        {
            "src": "/language-animation.js",
            "headers": {
                "Content-Type": "application/javascript"
            },
            "dest": "/dist/language-animation.js"
        },
        {
            "src": "/(styles|styles2)\\.css",
            "headers": {
                "Content-Type": "text/css"
            },
            "dest": "/dist/$1.css"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}
