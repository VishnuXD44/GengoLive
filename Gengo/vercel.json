{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/bundle.js",
            "dest": "/dist/bundle.js"
        },
        {
            "src": "/styles.css",
            "dest": "/dist/styles.css"
        },
        {
            "src": "/styles2.css",
            "dest": "/dist/styles2.css"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/dist/assets/$1"
        },
        {
            "src": "/scripts/(.*)",
            "dest": "/dist/scripts/$1"
        },
        {
            "src": "/(.*)\\.html",
            "dest": "/dist/$1.html"
        },
        {
            "src": "/(.*)",
            "dest": "src/server.js"
        }
    ]
}
